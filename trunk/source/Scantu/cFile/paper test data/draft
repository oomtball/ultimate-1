//@ ltl invariant positive: [](AP(x == 0) ==> <>AP(x == 1));

/* Testcase from Threader's distribution. For details see:
   http://www.model.in.tum.de/~popeea/research/threader
*/
#include <stdio.h>
#include <pthread.h>
typedef unsigned long int pthread_t;

int flag1 = 0, flag2 = 0; // boolean flags
int turn; // integer variable to hold the ID of the thread whose turn is it
int x = 0; // boolean variable to test mutual exclusion

void *thr1(void *_) {
    flag1 = 1;
    turn = 1;
    int f21 = flag2;
    int t1 = turn;
    while (f21==1 && t1==1) {
        f21 = flag2;
        t1 = turn;
    };
    // begin: critical section
    x++;
    printf("%d******\n",x);
    // end: critical section
    flag1 = 0;
    return 0;
}

void *thr2(void *_) {
    flag2 = 1;
    turn = 0;
    int f12 = flag1;
    int t2 = turn;
    while (f12==1 && t2==0) {
        f12 = flag1;
        t2 = turn;
    };
    // begin: critical section
    x++;
    printf("%d*******\n",x);
    // end: critical section
    flag2 = 0;
    return 0;
}
  
int main() {
  pthread_t t1, t2;
  printf("%d*\n",x);
  pthread_create(&t1, 0, thr1, 0);
  printf("%d**\n",x);
  pthread_create(&t2, 0, thr2, 0);
  printf("%d***\n",x);
  pthread_join(t1, 0);
  printf("%d****\n",x);
  pthread_join(t2, 0);
  printf("%d*****\n",x);
  return 0;
}

[ThreadState@L50, ThreadState@L24, ThreadState@L37]accept_S2
再到
[ThreadState@L50, ThreadState@L24, ThreadState@L35-2]accept_S2
就會去之前到過的 state，就會 report violation

呼叫的順序為 start_search(so)->Dfs(1)->dfs(1)，當 Figure 1c 的第 27 行將 Buchi Automaton 的 state 放到 Stack，接著 Figure 1c 的第 28 行呼叫 dfs(1)，Figure 1b 第 8 行得到的 s 就是 Buchi Automaton 的 state，這樣跟原本認知的就產生衝突.
 s is an accepting state in G，所以在這 s 為 Buchi Automaton 的 state


